<div class="col-md-6">
    <div class="panel panel-default">
      <div class="panel-heading">Account Information</div>
      <div class="panel-body">
        <%= user_field.label :email, "Email Address" %>
        <%= user_field.email_field :email, class: "form-control" %>
        <br>
        <%= user_field.label :email, "Confirm Email" %>
        <%= user_field.email_field :email, class: "form-control" %>
        <br>
        <%= user_field.label :password, "Password" %>
        <%= user_field.password_field :password, class: "form-control" %>
        <br>
        <%= user_field.label :password, "Confirm Password" %>
        <%= user_field.password_field :password, class: "form-control" %>
        <br>
        <%= user_field.label :first_name, "First Name" %>
        <%= user_field.text_field :first_name, class: "form-control" %>
        <br>
        <%= user_field.label :last_name, "Last Name" %>
        <%= user_field.text_field :last_name, class: "form-control" %>
      </div>
    </div>
</div>